<form>

{{#each purchaseData}}

<div class="form-check">
  <input class="form-check-input" type="checkbox" value={{this.name}} id="flexCheckChecked" checked>
  <label class="form-check-label" for="flexCheckChecked">
    {{this.name}}
  </label>
</div>

{{/each}}
<button type="submit" class="btn btn-primary" id="did-i-use-btn">Submit</button>
</form>